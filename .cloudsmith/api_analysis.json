{
  "org_name": "CloudSmith-Release-Safety",
  "repo_name": "pet-nutrition-service",
  "repo_url": "https://github.com/CloudSmith-Release-Safety/pet-nutrition-service",
  "provided_apis": [
    {
      "type": "rest",
      "endpoint": "/nutrition",
      "method": "GET",
      "service_name": "NutritionService",
      "description": "Retrieves all pet nutrition facts",
      "file_path": "pet-nutrition-service/server.js",
      "responses": {
        "200": {
          "description": "Successfully fetched all nutrition facts",
          "content_type": "application/json",
          "schema": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "pet_type": "string",
                "facts": "string"
              }
            }
          }
        },
        "500": {
          "description": "Failed to fetch nutrition facts",
          "content_type": "application/json",
          "schema": {
            "type": "object",
            "properties": {
              "message": "string",
              "error": "object"
            }
          }
        }
      }
    },
    {
      "type": "rest",
      "endpoint": "/nutrition/{pet_type}",
      "method": "GET",
      "service_name": "NutritionService",
      "description": "Retrieves nutrition facts for a specific pet type",
      "file_path": "pet-nutrition-service/server.js",
      "parameters": {
        "path": [
          {
            "name": "pet_type",
            "type": "string",
            "required": true,
            "description": "Type of pet (e.g., cat, dog, lizard)"
          }
        ]
      },
      "responses": {
        "200": {
          "description": "Successfully fetched nutrition fact for the specified pet type",
          "content_type": "application/json",
          "schema": {
            "type": "object",
            "properties": {
              "pet_type": "string",
              "facts": "string"
            }
          }
        },
        "404": {
          "description": "Nutrition fact not found for the given pet_type",
          "content_type": "application/json",
          "schema": {
            "type": "object",
            "properties": {
              "message": "string"
            }
          }
        },
        "500": {
          "description": "Failed to fetch nutrition fact",
          "content_type": "application/json",
          "schema": {
            "type": "object",
            "properties": {
              "message": "string",
              "error": "object"
            }
          }
        }
      }
    }
  ],
  "consumed_apis": [
    {
      "type": "rest",
      "service": "EurekaService",
      "endpoint": "/eureka/apps/{app}",
      "method": "POST",
      "description": "Registers the nutrition service with Eureka service registry",
      "file_path": "pet-nutrition-service/eureka-client.js",
      "request_payload": {
        "instance": {
          "hostName": "string",
          "instanceId": "string",
          "vipAddress": "string",
          "app": "string",
          "ipAddr": "string",
          "status": "string",
          "port": {
            "$": "number",
            "@enabled": "boolean"
          },
          "dataCenterInfo": {
            "@class": "string",
            "name": "string"
          }
        }
      }
    },
    {
      "type": "rest",
      "service": "EurekaService",
      "endpoint": "/eureka/apps/{app}/{instanceId}",
      "method": "PUT",
      "description": "Sends heartbeat to Eureka to maintain registration",
      "file_path": "pet-nutrition-service/eureka-client.js"
    }
  ]
}